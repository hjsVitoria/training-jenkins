pipeline {
	agent {
    		dockerfile {
			filename '04 - Dockerfile - Multiple Agents using Dockerfile'
			dir '003 - Defining Execution Environments'
			additionalBuildArgs ''
			args ''
		}
	}
    stages {
        stage('build') {
            steps {
            	echo "Don't install curl here!"
                sh "curl www.google.com"
            }
        }
    }
}
